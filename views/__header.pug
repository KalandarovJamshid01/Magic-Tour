//Header Section start
header.header 
    nav.nav.nav--tours
        a(href='/').nav__el All Tours 
        form(action="#").nav__search
            button.nav__search-btn 
                svg 
                    use(xlink:href="img/icons.svg#icon-search")
            input(type="text" placeholder="Search Tours").nav__search-input
    div.header__logo
        img(src="/img/logo-white.png", alt="Natour Logo") 
    nav.nav.nav--user
        a(href="/login").nav__el Log In 
        button.nav__el.nav__el--cta Sign Up 